<ul class="invoice_details_group unstyle-list">
    {% for item in items %}
        {% if item.value is not empty %}
            <li class="d-flex justify-content-between">
                {% if item.label is not empty %}
                    <label>{{ item.label|raw }}:</label>
                {% endif %}
                <strong>
                    {{ item.value|raw }}
                </strong>
            </li>
        {% endif %}
    {% endfor %}
    <div class="clear"></div>
</ul>

